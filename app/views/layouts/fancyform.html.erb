<!DOCTYPE html>
<html>
<head>

  <title><%= title %></title>

  <%= javascript_include_tag "application" %>
  <%= stylesheet_link_tag "application" %>

  <%= csrf_meta_tags %>
	<%= render 'layouts/stylesheets' %>
	<%= render 'layouts/google_analytics' %>
	



</head>
<body>
    <div class="container">


      <section class="round">
        <% flash.each do |key, value| %>
          <div class="flash <%= key %>"><%= value %></div>
        <% end %>
    	<%= yield %>

      </section>
     
    </div>

  </body>
</html>
